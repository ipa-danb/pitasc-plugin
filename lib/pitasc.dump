{"script": "<!-- An executable script -->\n<clone id=\"instance_script\" prototype=\"script\">\n</clone>\n", "script_action_caller": "<!-- A prototype for action caller scripts -->\n<clone id=\"instance_script_action_caller\" prototype=\"script_action_caller\">\n\t<!--member: namespace type: string -->\n\t<member id=\"namespace\"></member>\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: wait_until_done type: bool -->\n\t<member id=\"wait_until_done\">True</member>\n</clone>\n", "script_service_caller": "<!-- Prototype for service caller scripts -->\n<clone id=\"instance_script_service_caller\" prototype=\"script_service_caller\">\n\t<!--member: service_name type: string -->\n\t<member id=\"service_name\"></member>\n\t<!--member: namespace type: string -->\n\t<member id=\"namespace\"></member>\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: wait_for_service type: bool -->\n\t<member id=\"wait_for_service\">True</member>\n</clone>\n", "script_call_empty_srvs": "<!-- Calls a srvs with type 'std_msgs/Empty'. -->\n<clone id=\"instance_script_call_empty_srvs\" prototype=\"script_call_empty_srvs\">\n\t<!--member: service_name type: string -->\n\t<member id=\"service_name\"></member>\n\t<!--member: namespace type: string -->\n\t<member id=\"namespace\"></member>\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: wait_for_service type: bool -->\n\t<member id=\"wait_for_service\">True</member>\n</clone>\n", "script_call_trigger_srvs": "<!-- Calls a srvs with type 'std_msgs/Trigger'. -->\n<clone id=\"instance_script_call_trigger_srvs\" prototype=\"script_call_trigger_srvs\">\n\t<!--member: service_name type: string -->\n\t<member id=\"service_name\"></member>\n\t<!--member: namespace type: string -->\n\t<member id=\"namespace\"></member>\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: wait_for_service type: bool -->\n\t<member id=\"wait_for_service\">True</member>\n\t<!--member: retries type: int -->\n\t<member id=\"retries\">0</member>\n\t<!--member: throw_if_failed type: string -->\n\t<member id=\"throw_if_failed\">False</member>\n</clone>\n", "script_call_setbool_srvs": "<!-- Calls a srvs with type 'std_msgs/SetBool'. -->\n<clone id=\"instance_script_call_setbool_srvs\" prototype=\"script_call_setbool_srvs\">\n\t<!--member: service_name type: string -->\n\t<member id=\"service_name\"></member>\n\t<!--member: namespace type: string -->\n\t<member id=\"namespace\"></member>\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: wait_for_service type: bool -->\n\t<member id=\"wait_for_service\">True</member>\n\t<!--member: value type: bool -->\n\t<member id=\"value\">None</member>\n\t<!--member: retries type: int -->\n\t<member id=\"retries\">0</member>\n</clone>\n", "script_kinematic_graph": "<!-- Prototype for kinematic graph scripts -->\n<clone id=\"instance_script_kinematic_graph\" prototype=\"script_kinematic_graph\">\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: tf_broadcast type: bool -->\n\t<member id=\"tf_broadcast\">False</member>\n</clone>\n", "script_copy_frame": "<!-- Copies a frame. -->\n<clone id=\"instance_script_copy_frame\" prototype=\"script_copy_frame\">\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: tf_broadcast type: bool -->\n\t<member id=\"tf_broadcast\">False</member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n\t<!--member: source type: string -->\n\t<member id=\"source\"></member>\n\t<!--member: parent type: string -->\n\t<member id=\"parent\"></member>\n</clone>\n", "script_temp_frame": "<!-- Creates a temporary frame that lives as long as the parent skill is active. -->\n<clone id=\"instance_script_temp_frame\" prototype=\"script_temp_frame\">\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: tf_broadcast type: bool -->\n\t<member id=\"tf_broadcast\">False</member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n\t<!--member: source type: string -->\n\t<member id=\"source\"></member>\n\t<!--member: parent type: string -->\n\t<member id=\"parent\"></member>\n</clone>\n", "script_remove_frame": "<!-- Removes a frame. -->\n<clone id=\"instance_script_remove_frame\" prototype=\"script_remove_frame\">\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: tf_broadcast type: bool -->\n\t<member id=\"tf_broadcast\">False</member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n</clone>\n", "script_set_frame": "<!-- Sets a new frame. -->\n<clone id=\"instance_script_set_frame\" prototype=\"script_set_frame\">\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: tf_broadcast type: bool -->\n\t<member id=\"tf_broadcast\">False</member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n\t<!--member: parent type: string -->\n\t<member id=\"parent\"></member>\n\t<!--member: pose type: csv:float -->\n\t<member id=\"pose\">None</member>\n</clone>\n", "script_temp_frame_from_force": "<!-- Creates a temporary frame from the current force measurements.\n\n                    The z axis points in the opposite direction of the measured force vector (the usual definition for force control with pitasc).\n                    The frame's position and direction will be updated as long as the script is active.\n\n                    Includes possibility to filter the input with a first order low pass.\n                 -->\n<clone id=\"instance_script_temp_frame_from_force\" prototype=\"script_temp_frame_from_force\">\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: tf_broadcast type: bool -->\n\t<member id=\"tf_broadcast\">False</member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n\t<!--member: source type: string -->\n\t<member id=\"source\"></member>\n\t<!--member: parent type: string -->\n\t<member id=\"parent\">None</member>\n\t<!--member: low_pass_filter_gain type: float -->\n\t<member id=\"low_pass_filter_gain\">1.0</member>\n</clone>\n", "script_sinus_frame": "<!-- Creates a temporary frame that lives as long as the parent skill is active. The frame moves in the given coordinate with a sinus -->\n<clone id=\"instance_script_sinus_frame\" prototype=\"script_sinus_frame\">\n\t<!--member: on_start type: bool -->\n\t<member id=\"on_start\">True</member>\n\t<!--member: tf_broadcast type: bool -->\n\t<member id=\"tf_broadcast\">False</member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n\t<!--member: source type: string -->\n\t<member id=\"source\"></member>\n\t<!--member: parent type: string -->\n\t<member id=\"parent\"></member>\n\t<!--member: coordinate type: string -->\n\t<member id=\"coordinate\"></member>\n\t<!--member: amplitude type: float -->\n\t<member id=\"amplitude\">None</member>\n\t<!--member: period type: float -->\n\t<member id=\"period\">None</member>\n</clone>\n", "script_residual_publisher": "<!-- Publishes residual values (control error). -->\n<clone id=\"instance_script_residual_publisher\" prototype=\"script_residual_publisher\">\n\t<!--member: topic type: string -->\n\t<member id=\"topic\">residuals</member>\n</clone>\n", "script_measurement_publisher": "<!-- Publishes measurement values. -->\n<clone id=\"instance_script_measurement_publisher\" prototype=\"script_measurement_publisher\">\n\t<!--member: topic type: string -->\n\t<member id=\"topic\">measurements</member>\n\t<!--member: coordinates type: csv:string -->\n\t<member id=\"coordinates\">None</member>\n</clone>\n", "script_measurement_logger": "<!-- An executable script -->\n<clone id=\"instance_script_measurement_logger\" prototype=\"script_measurement_logger\">\n\t<!--member: file_name type: string -->\n\t<member id=\"file_name\"></member>\n\t<!--member: buffer_length type: int -->\n\t<member id=\"buffer_length\">512</member>\n\t<!--member: filter type: csv:string -->\n\t<member id=\"filter\"></member>\n\t<!--member: use_simple_filter_keys type: bool -->\n\t<member id=\"use_simple_filter_keys\">False</member>\n</clone>\n", "script_error_logger": "<!-- Logs control_errors (desired vs. measured values) -->\n<clone id=\"instance_script_error_logger\" prototype=\"script_error_logger\">\n\t<!--member: file_name type: string -->\n\t<member id=\"file_name\"></member>\n\t<!--member: buffer_length type: int -->\n\t<member id=\"buffer_length\">512</member>\n\t<!--member: filter type: csv:string -->\n\t<member id=\"filter\"></member>\n\t<!--member: use_simple_filter_keys type: bool -->\n\t<member id=\"use_simple_filter_keys\">False</member>\n</clone>\n", "script_status_publisher": "<!-- Publishes the maximum residual scaled between 0 and 1. -->\n<clone id=\"instance_script_status_publisher\" prototype=\"script_status_publisher\">\n\t<!--member: topic type: string -->\n\t<member id=\"topic\">status</member>\n\t<!--member: coordinates type: csv:string -->\n\t<member id=\"coordinates\">None</member>\n</clone>\n", "modifier": "<!-- An executable script -->\n<clone id=\"instance_modifier\" prototype=\"modifier\">\n\t<!--member: coordinate type: string -->\n\t<member id=\"coordinate\"></member>\n\t<!--member: prefix type: string -->\n\t<member id=\"prefix\"></member>\n</clone>\n", "script_desired_value_sine_modifier": "<!-- Modifies desired values using a temporal sine curve. -->\n<clone id=\"instance_script_desired_value_sine_modifier\" prototype=\"script_desired_value_sine_modifier\">\n\t<!--member: coordinate type: string -->\n\t<member id=\"coordinate\"></member>\n\t<!--member: prefix type: string -->\n\t<member id=\"prefix\"></member>\n\t<!--member: amplitude type: float -->\n\t<member id=\"amplitude\">1.0</member>\n\t<!--member: frequency type: float -->\n\t<member id=\"frequency\">1.0</member>\n\t<!--member: phase type: float -->\n\t<member id=\"phase\">0.0</member>\n</clone>\n", "script_desired_value_step_modifier": "<!-- Increases or decreases desired values in steps. -->\n<clone id=\"instance_script_desired_value_step_modifier\" prototype=\"script_desired_value_step_modifier\">\n\t<!--member: coordinate type: string -->\n\t<member id=\"coordinate\"></member>\n\t<!--member: prefix type: string -->\n\t<member id=\"prefix\"></member>\n\t<!--member: start_value type: float -->\n\t<member id=\"start_value\">None</member>\n\t<!--member: step_size type: float -->\n\t<member id=\"step_size\">None</member>\n</clone>\n", "modifier_task_desired_value_step": "<!-- Increases or decreases desired values in steps. -->\n<clone id=\"instance_modifier_task_desired_value_step\" prototype=\"modifier_task_desired_value_step\">\n\t<!--member: coordinate type: string -->\n\t<member id=\"coordinate\"></member>\n\t<!--member: prefix type: string -->\n\t<member id=\"prefix\"></member>\n\t<!--member: start_value type: float -->\n\t<member id=\"start_value\">None</member>\n\t<!--member: step_size type: float -->\n\t<member id=\"step_size\">None</member>\n\t<!--member: skip_steps type: int -->\n\t<member id=\"skip_steps\">0</member>\n\t<!--member: reset_steps type: int -->\n\t<member id=\"reset_steps\">-1</member>\n</clone>\n", "modifier_relative": "<!-- Calculates the target pose for relative target values. -->\n<clone id=\"instance_modifier_relative\" prototype=\"modifier_relative\">\n\t<!--member: coordinates type: csv:string -->\n\t<member id=\"coordinates\">None</member>\n\t<!--member: offsets type: csv:float -->\n\t<member id=\"offsets\">None</member>\n</clone>\n", "script_desired_value_random_modifier": "<!-- Modifies desired values using a AWN random value curve. -->\n<clone id=\"instance_script_desired_value_random_modifier\" prototype=\"script_desired_value_random_modifier\">\n\t<!--member: coordinate type: string -->\n\t<member id=\"coordinate\"></member>\n\t<!--member: minimum type: float -->\n\t<member id=\"minimum\">-0.01</member>\n\t<!--member: maximum type: float -->\n\t<member id=\"maximum\">0.01</member>\n\t<!--member: seed type: float -->\n\t<member id=\"seed\">43.0</member>\n</clone>\n", "script_desired_value_random_time_modifier": "<!-- Modifies desired values using a AWN random value curve. Fires event after a random time -->\n<clone id=\"instance_script_desired_value_random_time_modifier\" prototype=\"script_desired_value_random_time_modifier\">\n\t<!--member: coordinate type: string -->\n\t<member id=\"coordinate\"></member>\n\t<!--member: minimum type: float -->\n\t<member id=\"minimum\">-0.01</member>\n\t<!--member: maximum type: float -->\n\t<member id=\"maximum\">0.01</member>\n\t<!--member: seed type: float -->\n\t<member id=\"seed\">43.0</member>\n\t<!--member: min_time type: float -->\n\t<member id=\"min_time\">-0.1</member>\n\t<!--member: max_time type: float -->\n\t<member id=\"max_time\">2.0</member>\n\t<!--member: time_event type: string -->\n\t<member id=\"time_event\">timeisup</member>\n</clone>\n", "script_topic_logger": "<!-- Loggs values from a ROS topic. -->\n<clone id=\"instance_script_topic_logger\" prototype=\"script_topic_logger\">\n\t<!--member: topic type: string -->\n\t<member id=\"topic\"></member>\n\t<!--member: package type: string -->\n\t<member id=\"package\"></member>\n\t<!--member: folder type: string -->\n\t<member id=\"folder\"></member>\n\t<!--member: file_name type: string -->\n\t<member id=\"file_name\"></member>\n\t<!--member: file_mode type: string -->\n\t<member id=\"file_mode\">w+</member>\n</clone>\n", "script_external_topic_logger": "<!-- Loggs values from a ROS topic. -->\n<clone id=\"instance_script_external_topic_logger\" prototype=\"script_external_topic_logger\">\n\t<!--member: topic type: string -->\n\t<member id=\"topic\"></member>\n\t<!--member: package type: string -->\n\t<member id=\"package\"></member>\n\t<!--member: folder type: string -->\n\t<member id=\"folder\"></member>\n\t<!--member: file_name type: string -->\n\t<member id=\"file_name\"></member>\n\t<!--member: file_mode type: string -->\n\t<member id=\"file_mode\">w+</member>\n\t<!--member: name type: string -->\n\t<member id=\"name\"></member>\n</clone>\n", "monitor": "<!-- Monitors specified data -->\n<clone id=\"instance_monitor\" prototype=\"monitor\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n</clone>\n", "monitor_nop": "<!-- Monitor that never fires -->\n<clone id=\"instance_monitor_nop\" prototype=\"monitor_nop\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n</clone>\n", "monitor_sync": "<!-- Contains other monitors and triggers when all sub-events match. -->\n<clone id=\"instance_monitor_sync\" prototype=\"monitor_sync\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: monitors type: List : Enter monitors here -->\n\t<member id=\"monitors\">\n\t</member>\n\t<!--member: events type: csv:string -->\n\t<member id=\"events\">None</member>\n</clone>\n", "monitor_cycle_counter": "<!-- Triggers after a certain number of cycles. -->\n<clone id=\"instance_monitor_cycle_counter\" prototype=\"monitor_cycle_counter\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: max_count type: int -->\n\t<member id=\"max_count\">None</member>\n</clone>\n", "monitor_execution_counter": "<!-- Triggers after a certain number of executions of the parent skill. -->\n<clone id=\"instance_monitor_execution_counter\" prototype=\"monitor_execution_counter\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: max_count type: int -->\n\t<member id=\"max_count\">None</member>\n</clone>\n", "monitor_duration": "<!-- Triggers after a certain time. -->\n<clone id=\"instance_monitor_duration\" prototype=\"monitor_duration\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: duration type: float -->\n\t<member id=\"duration\">None</member>\n</clone>\n", "monitor_script_result": "<!-- Triggers when a script returns a certain result. -->\n<clone id=\"instance_monitor_script_result\" prototype=\"monitor_script_result\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: result type: string -->\n\t<member id=\"result\"></member>\n</clone>\n", "monitor_trigger_script_result": "<!-- Triggers when a script calling a trigger-like service finishes. -->\n<clone id=\"instance_monitor_trigger_script_result\" prototype=\"monitor_trigger_script_result\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: fail_event type: string -->\n\t<member id=\"fail_event\">preempted</member>\n</clone>\n", "monitor_wait_for_transform": "<!-- Triggers when a frame becomes available. -->\n<clone id=\"instance_monitor_wait_for_transform\" prototype=\"monitor_wait_for_transform\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n\t<!--member: parent_frame type: string -->\n\t<member id=\"parent_frame\">world</member>\n</clone>\n", "monitor_distance": "<!-- Triggers when each frame's coordinate relative to a reference frame exceeds a certain threshold.\n\n                Every coordinate is handled individually. Triggers when true for every coordinate.\n                 -->\n<clone id=\"instance_monitor_distance\" prototype=\"monitor_distance\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: reference_frame type: string -->\n\t<member id=\"reference_frame\"></member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n\t<!--member: coordinates type: csv:string -->\n\t<member id=\"coordinates\">None</member>\n\t<!--member: distance_coordinates type: csv:string -->\n\t<member id=\"distance_coordinates\">None</member>\n\t<!--member: distances type: csv:float -->\n\t<member id=\"distances\">None</member>\n\t<!--member: target_offsets type: csv:float -->\n\t<member id=\"target_offsets\"></member>\n\t<!--member: operator type: string -->\n\t<member id=\"operator\"></member>\n</clone>\n", "monitor_relative_distance": "<!-- Triggers when a frame moved a certain relative distance. -->\n<clone id=\"instance_monitor_relative_distance\" prototype=\"monitor_relative_distance\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: reference_frame type: string -->\n\t<member id=\"reference_frame\"></member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n\t<!--member: coordinates type: csv:string -->\n\t<member id=\"coordinates\">None</member>\n\t<!--member: distance_coordinates type: csv:string -->\n\t<member id=\"distance_coordinates\">None</member>\n\t<!--member: distances type: csv:float -->\n\t<member id=\"distances\">None</member>\n\t<!--member: target_offsets type: csv:float -->\n\t<member id=\"target_offsets\"></member>\n\t<!--member: operator type: string -->\n\t<member id=\"operator\"></member>\n</clone>\n", "monitor_ptp_distance": "<!-- Fires when the absolute distances between the current joint states and the target joints states are all below the given distances values.\n                 -->\n<clone id=\"instance_monitor_ptp_distance\" prototype=\"monitor_ptp_distance\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: target_joint_states type: csv:float -->\n\t<member id=\"target_joint_states\">None</member>\n\t<!--member: coordinates type: csv:string -->\n\t<member id=\"coordinates\">None</member>\n\t<!--member: distances type: csv:float -->\n\t<member id=\"distances\">None</member>\n</clone>\n", "monitor_frame_in_box": "<!-- Check whether a frame lies inside a bounding box. -->\n<clone id=\"instance_monitor_frame_in_box\" prototype=\"monitor_frame_in_box\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: reference_frame type: string -->\n\t<member id=\"reference_frame\"></member>\n\t<!--member: coordinates type: csv:string -->\n\t<member id=\"coordinates\">x, y, z</member>\n\t<!--member: distance_coordinates type: csv:string -->\n\t<member id=\"distance_coordinates\">x, y, z</member>\n\t<!--member: distances type: csv:float -->\n\t<member id=\"distances\">None</member>\n\t<!--member: target_offsets type: csv:float -->\n\t<member id=\"target_offsets\"></member>\n\t<!--member: operator type: string -->\n\t<member id=\"operator\">absolute_less_equal</member>\n\t<!--member: frame type: string -->\n\t<member id=\"frame\"></member>\n\t<!--member: box_center_frame type: string -->\n\t<member id=\"box_center_frame\"></member>\n\t<!--member: edge_length_xyz type: csv:float -->\n\t<member id=\"edge_length_xyz\">None</member>\n</clone>\n", "monitor_ros_service": "<!-- Prototype for ROS service monitors -->\n<clone id=\"instance_monitor_ros_service\" prototype=\"monitor_ros_service\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: namespace type: string -->\n\t<member id=\"namespace\"></member>\n\t<!--member: service_name type: string -->\n\t<member id=\"service_name\"></member>\n</clone>\n", "monitor_action_caller": "<!-- A prototype for action caller monitor -->\n<clone id=\"instance_monitor_action_caller\" prototype=\"monitor_action_caller\">\n\t<!--member: event type: string -->\n\t<member id=\"event\">succeeded</member>\n\t<!--member: namespace type: string -->\n\t<member id=\"namespace\"></member>\n\t<!--member: action_name type: string -->\n\t<member id=\"action_name\"></member>\n</clone>\n", "monitor_ros_empty_service": "<!-- Triggers when a ROS Empty service is called -->\n<clone id=\"instance_moni